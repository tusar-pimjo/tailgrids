{
  "name": "@tailgrids/cli",
  "type": "module",
  "version": "1.0.1",
  "description": "CLI for adding Tailgrids components to React projects.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "tailgrids": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsup --watch",
    "dev:link": "npm run build && npm link --local",
    "prebuild": "npm run registry:build",
    "build": "tsup",
    "registry:build": "tsx ./scripts/build-registries.ts && prettier --log-level silent --write ./registries.json"
  },
  "keywords": [
    "tailgrids",
    "cli",
    "tailwind",
    "ui"
  ],
  "author": {
    "name": "Tailgrids",
    "url": "https://tailgrids.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tailgrids/tailgrids.git",
    "directory": "/packages/cli"
  },
  "homepage": "https://tailgrids.com",
  "devDependencies": {
    "@repo/typescript-config": "*",
    "@types/node": "^22.15.3",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "tsx": "^4.20.6"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.8.6",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "enquirer": "^2.4.1",
    "ora": "^9.0.0"
  }
}
